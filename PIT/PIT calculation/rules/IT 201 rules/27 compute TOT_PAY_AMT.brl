  <name>27 compute TOT_PAY_AMT</name>
  <uuid>c7ee0a94-0379-4e46-9651-15c45cd51152</uuid>
  <locale>en_US</locale>
  <definition><![CDATA[then
	set the value of the TOT_PAY_AMT of IT201 to 
    		the value of the ESC_CHLD_CR_AMT of IT201 + 
        	the value of the NYS_NYC_CDCC_AMT of IT201 + 
        	the value of the EITC_CR_AMT of IT201 +
        	the value of the NC_P_EITC_CR_AMT of IT201 +
            the value of the RL_PROP_CR_AMT of IT201 +
            the value of the CLGE_TUIT_CR_AMT of IT201 +
            the value of the NYC_STAR_CR_AMT of IT201 +
            the value of the NYC_STAR_REDCR_AMT of IT201 +
            the value of the NYC_EITC_CR_AMT of IT201 +
            the value of the OTH_RFND_CR_AMT of IT201 +
            the value of the TOT_WTHLD_AMT of IT201 +
            the value of the TOT_NYC_WTHLD_AMT of IT201 +
            the value of the TOT_YNK_WTHLD_AMT of IT201 +
            the value of the TOT_EST_TX_AMT of IT201;  ]]></definition>
</ilog.rules.studio.model.brl:ActionRule>
